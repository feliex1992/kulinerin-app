{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "no-restricted-globals": "off",
        "no-useless-return": "off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "eqeqeq": "off",
        "no-plusplus": "off"
    }
}
